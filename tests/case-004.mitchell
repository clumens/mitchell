# Description:  Nested case expressions
# Expected:  PASS

ℳ Main ← decl
   ʋ something: integer ← 0

   ƒ asdf: boolean () ← t

   ƒ foo: integer () ←
      case Main.asdf() in
         t → case something in
                1 → 1,
                0 → 0
             end,
         f → 1
      end
end
