# Description:  Nested case expressions
# Expected:  PASS

ℳ Main ← decl
   ʋ something: integer ← 0
   ʋ otherthing: boolean ← f

   ƒ asdf: boolean () ← t

   ƒ foo: integer () ←
      case Main.asdf() in
         t → case something in
                1 → 1,
                0 → 0,
                else → 47
             end,
         otherthing → 1,
         else → 100
      end
end
